Fn[right, $>:[
    ~>n = [:Num[:input["number: "]]],
    
    Fn[n, right, @:~>right < [@:~>n] (then-else) [
        $:print['Too large!'],
        @:~>right > [@:~>n] (then-else) [
            $:print['Too low!'],
            $>:[print['Exactly!'], exit[]]
        ]
    ]] [@:~>n, @:~>right],
    ~>__fn__(async)[@:~>right]
]] [
    :import['@random'] from_range[:0...[:100]]
]
